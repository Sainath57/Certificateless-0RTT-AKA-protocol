	<div class="all-title-box">
	<div>
		<%
		session = request.getSession(false);
		//String uid = (String) session.getAttribute("email");
		String cspemail=(String)session.getAttribute("cspemail");
		String sql = "select * from csppk where  cid='"+cspemail+"'";
		List<String> lt = DBConnection.getCSPPrivateKey(sql);
		Iterator<String> itr = lt.iterator();
		%>
				<div class="inner" >
				<table width="100%" style="text-align: center;">
				<tr><th style="text-align: center;color: white;">CID</th>
				<th style="text-align: center;color: white;">Private Key</th>
				<th style="text-align: center;color: white;">Update Key</th>
				</tr>
				<%
				while(itr.hasNext()){
					String u = itr.next();
					String m = itr.next();
				%>
				<tr><td style="text-align: center;color: white;"><%=u %></td>
				<td style="text-align: center;color: white;"><%=m %></td>
				<td><a href="CSPPKUpdate?cspemail=<%=cspemail%>"  style="text-align: center;color: white;" class="actions">Update</a></td>
				</tr>
				<%} %>
				</table>
				</div>
		</div>
	</div>

